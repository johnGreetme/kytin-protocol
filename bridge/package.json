{
  "name": "@kytin/bridge",
  "version": "1.0.0",
  "description": "TypeScript bridge for Kytin Protocol - Hardware Root of Trust for AI Agents",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "lint": "eslint src/",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "kytin",
    "tpm",
    "hardware-security",
    "ai-agents",
    "solana",
    "depin",
    "state-locked-protocol",
    "clawhub"
  ],
  "author": "Kytin Protocol",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kytin-protocol/kytin"
  },
  "homepage": "https://kytin.io",
  "bugs": {
    "url": "https://github.com/kytin-protocol/kytin/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist",
    "SKILL.md"
  ],
  "devDependencies": {
    "@types/node": "^20.0.0",
    "eslint": "^8.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "zod": "^3.22.0",
    "tweetnacl": "^1.0.3"
  },
  "peerDependencies": {
    "@solana/web3.js": "^1.90.0"
  },
  "peerDependenciesMeta": {
    "@solana/web3.js": {
      "optional": true
    }
  }
}
